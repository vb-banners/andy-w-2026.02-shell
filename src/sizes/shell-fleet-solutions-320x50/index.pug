extends ../../global/master
include ../../global/set

block size
    - var width = '320', height = '50'

block js
    script.
        gsap.set('.banner', {visibility: 'visible'});
        gsap.set('.banner *', {z: 0.01});

        gsap.set('.fg', {transformOrigin: '50% 50%', scale: 1.1});

        let expo = CustomEase.create('custom', 'M0,0,C0.084,0.61,0.214,0.802,0.28,0.856,0.356,0.918,0.374,1,1,1');
        let expoInOut = CustomEase.create('custom', 'M0,0,C0.25,0,0.294,0.023,0.335,0.05,0.428,0.11,0.466,0.292,0.498,0.502,0.532,0.73,0.586,0.88,0.64,0.928,0.679,0.962,0.698,1,1,1');
        let expoIn = CustomEase.create('custom', 'M0,0,C0.5,0,0.581,0.047,0.625,0.073,0.72,0.13,0.9,0.23,1,1');

        let loop = 0, loopMax = 2,
        master = gsap.timeline({delay: 0.25, repeat: loopMax - 1, defaults: {duration: 1, ease: expo}})
            .from('.txt1', {autoAlpha: 0}, 0)
                .from('.pic', {duration: 3, transformOrigin: '50% 50%', scale: 1.1, ease: 'none'}, '<')

            .from('.pic-mask', {duration: 1.25, transformOrigin: '50% 50%', scale: 4.25, x: '-=75', ease: 'back.inOut(1.25)'}, '>-=100%')
                .to('.pic-container', {duration: 1.25, transformOrigin: '50% 50%', scale: 0.9, ease: 'back.inOut(1.25)'}, '<')
                .to('.pic', {duration: 1.25, x: '+=3', ease: 'back.inOut(1.25)'}, '<')
            
            .from('.yellow-circle', {transformOrigin: '50% 50%', scale: 0, ease: 'back(1.2)'}, '<50%')
            .from('.red-circle', {transformOrigin: '50% 50%', scale: 0, ease: 'back(1.2)'}, '<35%')
            .from('.circle', {transformOrigin: '50% 50%', scale: 0, stagger: -0.1, ease: 'back(1.2)'}, '<25%')
                .to('.shadow-circle', {autoAlpha: 0, stagger: -0.1, ease: 'back(1.2)'}, '<50%')

            .from('.logo, .txt2', {autoAlpha: 0}, '<25%')
            .from('.cta-btn', {y: '+=100'}, '>')

            .to('.cta-btn', {duration: 0.275, transformOrigin: '50% 50%', scale: 1.1, yoyo: true, repeat: 3, ease: 'sine.inOut', yoyoEase: 'sine.inOut'}, '>0.5')

            .call(() => {loop++; loop < loopMax ? master.play() : master.pause()}, null, 14.5)
                .from('.fg', {autoAlpha: 0, ease: expoIn}, '>')
        ;

        window.exposeMaster && window.exposeMaster(master);

